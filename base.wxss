
text,view{
  font-family: "OpenSansRegular";
  font-size: 28rpx;
}
/* 图片大小 */
.sm_icon{
  width: 45rpx;
  height: 45rpx;
}
.mid_icon{
  width: 75rpx;
  height: 75rpx;
}
.lag_icon{
  width: 100rpx;
  height: 100rpx;
}
/* 居中 */
.w-vertical-align-l{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}
.w-vertical-align-t{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.w-vertical-align-c{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
/* 按钮 */
button::after{
  border: 0;
}
button{
  margin: 0;
  padding: 0;
  background: rgba(255, 255, 255, 0);
  border-radius: 0;
}
.button-hover {
  color:rgba(0, 0, 0, 0);
  background-color: rgba(0, 0, 0, 0);
}
.btnBg{
   background: linear-gradient(to right, #FD3174,#FD5A82, #FE7DAA);
}
.fontColor {
  color: #B51748;
}

.loadmore {
	display: inline-block;
    vertical-align: middle;
    font-weight: normal;
    font-size: 14px;
}

/* 加载中 start */
.loading {
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  animation: loading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.text {
	display: inline-block;
    vertical-align: middle;
}

@keyframes loading {
  0% {
      transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
      transform: rotate3d(0, 0, 1, 360deg);
  }
}

